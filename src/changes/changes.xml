<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">
	<properties>
		<title>Changes</title>
		<author email="ivo.maixner@gmail.com">Ivo Maixner</author>
	</properties>
	<body>
		<release date="2013-12-14" version="2.0.0"
			description="Token @auto. Profiles. Configurable properties file location">
			<action type="add">Add token @auto.</action>
			<action type="add">Add support for profiles - connections to
				multiple databases.
			</action>
			<action type="add">Enable configure location of Light Air properties
				file using system property.
			</action>
		</release>
		<release date="2013-11-24" version="1.0.0"
			description="Pool connections. Durations.">
			<action type="add">Add support for duration in temporal tokens.
			</action>
			<action type="add">Use datasource to pool database connections.
			</action>
			<action type="add">Add support for Informix.</action>
		</release>
		<release date="2013-09-15" version="0.5">
		</release>
	</body>
</document>
